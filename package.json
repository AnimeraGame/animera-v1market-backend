{
  "name": "marsverse-marketplace-be",
  "version": "0.0.1",
  "description": "marsverse-marketplace-be",
  "author": "Opsjet",
  "license": "ISC",
  "keywords": [
    "NestJS",
    "Prisma",
    "Prisma Client",
    "Typescript"
  ],
  "scripts": {
    "dev": "nest start --watch",
    "prebuild": "rimraf dist",
    "build": "npx prisma generate && nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/src/main",
    "migrate:dev": "prisma migrate dev --preview-feature",
    "migrate:dev:create": "prisma migrate dev --create-only --preview-feature",
    "migrate:reset": "prisma migrate reset --preview-feature",
    "migrate:deploy": "npx prisma migrate deploy --preview-feature",
    "migrate:status": "npx prisma migrate status --preview-feature",
    "migrate:resolve": "npx prisma migrate resolve --preview-feature",
    "prisma:studio": "npx prisma studio",
    "prisma:generate": "npx prisma generate",
    "prisma:generate:watch": "npx prisma generate --watch",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "start:db": "npm run migrate:up && npm run prisma:generate && npm run seed",
    "seed": "ts-node prisma/seed.ts",
    "postinstall": "npm run prisma:generate",
    "prepare": "husky install"
  },
  "dependencies": {
    "@devoxa/prisma-relay-cursor-connection": "2.0.2",
    "@nestjs/common": "^8.0.6",
    "@nestjs/config": "1.0.1",
    "@nestjs/core": "^8.0.6",
    "@nestjs/event-emitter": "^1.0.0",
    "@nestjs/graphql": "9.0.4",
    "@nestjs/jwt": "8.0.0",
    "@nestjs/passport": "8.0.1",
    "@nestjs/platform-express": "8.0.6",
    "@nestjs/platform-socket.io": "^8.0.6",
    "@nestjs/swagger": "5.0.9",
    "@nestjs/terminus": "^8.0.0",
    "@nestjs/websockets": "^8.0.6",
    "@nextnm/nestjs-ses": "0.0.4",
    "@truffle/hdwallet-provider": "^2.0.3",
    "@types/mime": "^2.0.3",
    "abi-decoder": "^2.4.0",
    "amqp-connection-manager": "^3.6.0",
    "amqplib": "^0.8.0",
    "apollo-server-core": "^3.3.0",
    "apollo-server-express": "3.3.0",
    "apollo-server-plugin-base": "^3.2.0",
    "apollo-server-plugin-operation-registry": "^3.2.0",
    "aws-sdk": "^2.983.0",
    "axios": "^0.24.0",
    "bcrypt": "5.0.1",
    "camelcase-keys": "^7.0.0",
    "class-transformer": "0.4.0",
    "class-validator": "^0.13.2",
    "csv-parser": "^3.0.0",
    "dotenv": "^10.0.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eth-crypto": "^2.2.0",
    "eth-sig-util": "^3.0.1",
    "ethereumjs-util": "^7.1.0",
    "ethers": "^5.5.3",
    "file-type": "^16.5.3",
    "graphql": "^15.5.3",
    "graphql-bigint": "^1.0.0",
    "graphql-subscriptions": "1.2.1",
    "graphql-tools": "8.2.0",
    "graphql-type-json": "^0.3.2",
    "graphql-upload": "^12.0.0",
    "iconv": "^3.0.0",
    "mime": "^2.5.2",
    "moment": "^2.29.1",
    "nestjs-s3": "^1.0.1",
    "node-fetch": "3.0",
    "node-schedule": "^2.0.0",
    "oauth": "^0.9.15",
    "object-path": "^0.11.7",
    "objects-to-csv": "^1.3.6",
    "passport": "0.4.1",
    "passport-jwt": "4.0.0",
    "passport-twitter-oauth2": "^2.1.1",
    "redis": "^3.1.2",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.3.0",
    "seedrandom": "^3.0.5",
    "swagger-ui-express": "4.1.6",
    "twit": "^2.2.4",
    "uid": "^2.0.0",
    "uuid": "^8.3.2",
    "web3": "^1.5.2",
    "web3-core": "^1.6.1",
    "web3-eth": "^1.6.1",
    "web3-eth-personal": "^1.5.2"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.1.5",
    "@nestjs/testing": "8.0.6",
    "@prisma/client": "^3.5.0",
    "@types/amqplib": "^0.8.2",
    "@types/bcrypt": "5.0.0",
    "@types/chance": "1.1.3",
    "@types/express": "4.17.13",
    "@types/graphql-upload": "^8.0.7",
    "@types/jest": "27.0.1",
    "@types/multer": "^1.4.7",
    "@types/node": "^16.7.10",
    "@types/node-fetch": "^3.0.3",
    "@types/passport-jwt": "^3.0.6",
    "@types/redis": "^2.8.31",
    "@types/supertest": "2.0.11",
    "@types/twit": "^2.2.30",
    "@types/uuid": "^8.3.1",
    "@typescript-eslint/eslint-plugin": "4.30.0",
    "@typescript-eslint/parser": "4.30.0",
    "chance": "1.1.8",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-typescript": "^14.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.25.1",
    "husky": "^7.0.2",
    "jest": "27.1.0",
    "lint-staged": "^11.1.2",
    "prettier": "2.3.2",
    "prettier-eslint": "^8.2.2",
    "prisma": "^3.4.2",
    "prisma-dbml-generator": "^0.8.2",
    "rimraf": "3.0.2",
    "supertest": "6.1.6",
    "ts-jest": "27.0.5",
    "ts-loader": "9.2.5",
    "ts-node": "10.2.1",
    "tsconfig-paths": "3.11.0",
    "typescript": "^4.4.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write",
      "eslint --ext .ts --fix"
    ]
  }
}
